{
    "name": "chronolabs/clicky",
    "description": "Extension to add Clicky tracking tag into page",
    "type": "bolt-extension",
    "keywords": [
        "Clicky","Analytics","Tools"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Rodriguez",
            "email": "victor.rodriguez@chrono-labs.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\ChronoLabs\\Clicky\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\ChronoLabs\\Clicky\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\ChronoLabs\\Clicky\\ClickyExtension"
    }
}
